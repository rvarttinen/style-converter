<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<rendertheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" map-background="#f8f4f0" version="1" xsi:schemaLocation="http://opensciencemap.org/rendertheme https://raw.githubusercontent.com/mapsforge/vtm/master/resources/rendertheme.xsd">
      
    <!--###### TEXT styles ######-->
       
    <!--###### AREA styles ######-->
      
    <!--building-->
    <style-area id="building"/>
     
    <!--building-top-->
    <style-area fade="16" fill="#f2eae2" id="building-top"/>
      
    <!--###### LINE styles ######-->
      
    <!--building-top-->
     <style-line id="building-top" stroke="#dfdbd7" width="1"/>
      
    <!--###### SYMBOL styles ######-->
       
    <!--###### ASSIGNMENT ######-->
     
    <m k="layer" v="building">
        <m zoom-max="16">
            <area fade="16" fill="#dfdbd7" use="building"/>
            <line use="building-top"/>
            <area use="building-top"/>
        </m>
        <m zoom-max="15">
            <area fade="15" fill="#f2eae2" use="building"/>
            <line use="building-top"/>
            <area use="building-top"/>
        </m>
    </m>
</rendertheme>
